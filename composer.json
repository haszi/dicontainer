{
    "name": "haszi/dicontainer",
    "description": "A learning exercise of a PSR-11 compatible dependency injection container",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
          "haszi\\": "src/"
        }
    },
      "autoload-dev": {
        "psr-4": {
            "haszi\\DI\\Test\\UnitTest\\": "tests/Unit/"
        }
    },
    "require": {
        "php": ">=8.0",
        "psr/container": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4",
        "phpstan/phpstan": "^1.10",
        "vimeo/psalm": "^5.15"
    }
}
